    <item>
      <title>Fd_ctf</title>
      <link>http://localhost:1313/fd_ctf/</link>
      <pubDate>Tue, 10 Jun 2025 21:05:17 +0300</pubDate>
      <author>nuno@n9o.xyz (Blowfish)</author>
      <guid>http://localhost:1313/fd_ctf/</guid>
      <description>&lt;p&gt;this is how i solved the fd challenge on pwnable.kr&lt;/p&gt;
&lt;p&gt;char buf[32];
int main(int argc, char* argv[], char* envp[]){
if(argc&amp;lt;2){
printf(&amp;ldquo;pass argv[1] a number\n&amp;rdquo;);
return 0;
}
int fd = atoi( argv[1] ) - 0x1234;
int len = 0;
len = read(fd, buf, 32);
if(!strcmp(&amp;ldquo;LETMEWIN\n&amp;rdquo;, buf)){
printf(&amp;ldquo;good job :)\n&amp;rdquo;);
setregid(getegid(), getegid());
system(&amp;quot;/bin/cat flag&amp;quot;);
exit(0);
}
printf(&amp;ldquo;learn about Linux file IO\n&amp;rdquo;);
return 0;&lt;/p&gt;</description>
      
